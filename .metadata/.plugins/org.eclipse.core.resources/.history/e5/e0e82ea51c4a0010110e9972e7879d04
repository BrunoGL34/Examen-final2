spring.application.name=ResponsableMS

#configuracion inicial del proyecto
server.port = 8024

#Coneccion a oracle y configuracion de JPA
spring.datasource.url=jdbc:oracle:thin:@localhost:1521/xe
spring.datasource.username=ORCL
